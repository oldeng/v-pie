{
  "name": "@oldeng/v-pie",
  "version": "0.1.5",
  "main": "dist/v-pie.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --report",
    "lib": "vue-cli-service build --target lib --name v-pie ./src/components/v-pie/index.vue",
    "dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.es.js --watch",
    "build:lib": "cross-env NODE_ENV=production npm run build:browser && npm run build:es && npm run build:umd",
    "test": "npm run test:unit",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js"
  },
  "dependencies": {
    "@jiaminghi/color": "^0.1.1",
    "core-js": "^3.6.4",
    "vant": "^2.8.6",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "cross-env": "^6.0.3",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "rollup": "^1.27.5",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-css-only": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.11"
  }
}
